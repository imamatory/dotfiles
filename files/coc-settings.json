{
  "suggest.noselect": false,
  "diagnostic.infoSign": "？",
  "diagnostic.warningSign": "⚠",
  "diagnostic.errorSign": "✕",
  "diagnostic.messageTarget": "echo",
  "suggest.maxCompleteItemCount": 30,
  "elixir.pathToElixirLS": "~/.elixir-ls/release/language_server.sh",
  "eslint.autoFixOnSave": true,
  "python.jediEnabled": false,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": ["--config", "setup.cfg"],
  "python.linting.enabled": true,
  "python.sortImports.path": "isort",
  "python.autoComplete.extraPaths": ["./cc/"],
  "python.poetryPath": "~/.poetry/bin/poetry",
  "stylelintplus.autoFixOnSave": true,
  "coc.preferences.formatOnSaveFiletypes": ["css", "scss"],
  "languageserver": {
    "dart": {
      "command": "dart",
      "args": [
        "/home/im/.asdf/installs/dart/2.5.2/dart-sdk/bin/snapshots/analysis_server.dart.snapshot",
        "--lsp"
      ],
      "filetypes": ["dart"],
      "disableDynamicRegister": true,
      "trace.server": "verbose"
    },
    "haskell": {
      "command": "hie-wrapper",
      "rootPatterns": ["stack.yaml", "cabal.config", "package.yaml"],
      "filetypes": ["hs", "lhs", "haskell"],
      "initializationOptions": {
        "languageServerHaskell": {
          "hlintOn": true
        }
      }
    },
    "clojure-lsp": {
      "command": "bash",
      "args": ["-c", "clojure-lsp"],
      "filetypes": ["clojure"],
      "rootPatterns": ["project.clj", "deps.edn"],
      "additionalSchemes": ["jar", "zipfile"],
      "trace.server": "verbose",
      "initializationOptions": {}
    }
  }
}
