{
  "suggest.noselect": false,
  "diagnostic.infoSign": "？",
  "diagnostic.warningSign": "⚠",
  "diagnostic.errorSign": "✕",
  "suggest.maxCompleteItemCount": 30,
  "eslint.trace.server": "verbose",
  "eslint.packageManager": "yarn",
  "eslint.autoFix": true,
  "eslint.autoFixOnSave": false,
  "eslint.workingDirectories": [
    { "mode": "auto" }
  ],
  "eslint.filetypes": [
    "javascript",
    "javascript.jsx",
    "typescript",
    "typescript.jsx",
    "javascriptreact",
    "typescriptreact"
  ],
  "elixir.pathToElixirLS": "~/.elixir-ls/release/language_server.sh",
  "languageserver": {
    "dart": {
      "command": "dart",
      "args": [
        "/home/im/.asdf/installs/dart/2.5.2/dart-sdk/bin/snapshots/analysis_server.dart.snapshot",
        "--lsp"
      ],
      "filetypes": ["dart"],
      "disableDynamicRegister": true,
      "trace.server": "verbose"
    },
    "haskell": {
      "command": "hie-wrapper",
      "rootPatterns": [
        "stack.yaml",
        "cabal.config",
        "package.yaml"
      ],
      "filetypes": [
        "hs",
        "lhs",
        "haskell"
      ],
      "initializationOptions": {
        "languageServerHaskell": {
          "hlintOn": true
        }
      }
    },
    "clojure-lsp": {
      "command": "bash",
      "args": ["-c", "clojure-lsp"],
      "filetypes": ["clojure"],
      "rootPatterns": ["project.clj", "deps.edn"],
      "additionalSchemes": ["jar", "zipfile"],
      "trace.server": "verbose",
      "initializationOptions": {
      }
    }
  },
  "diagnostic-languageserver.filetypes": {
    "markdown": [
      "write-good",
      "markdownlint"
    ],
    "ruby": [
      "solargraph"
    ],
    "javascript": [
      "eslint"
    ],
    "typescript": [
      "eslint"
    ],
    "javascriptreact": [
      "eslint"
    ],
    "typescriptreact": [
      "eslint"
    ],
    "sh": "shellcheck"
  },
  "solargraph.diagnostics": true,
  "solargraph.hover": true,
  "solargraph.autoformat": true,
  "solargraph.useBundler": true,
  "solargraph.bundlerPath": "~/.rbenv/shims/bundle"
}
