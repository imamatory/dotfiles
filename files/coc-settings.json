{
  "suggest.noselect": false,
  "diagnostic.infoSign": "？",
  "diagnostic.warningSign": "⚠",
  "diagnostic.errorSign": "✕",
  "suggest.maxCompleteItemCount": 30,
  "eslint.trace.server": "verbose",
  "eslint.packageManager": "yarn",
  "eslint.autoFix": true,
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": [
    "javascript",
    "javascript.jsx",
    "typescript",
    "typescript.jsx",
    "javascriptreact",
    "typescriptreact"
  ],
  "languageserver": {
    "dart": {
      "command": "dart",
      "args": [
        "/home/im/.asdf/installs/dart/2.5.2/dart-sdk/bin/snapshots/analysis_server.dart.snapshot",
        "--lsp"
      ],
      "filetypes": ["dart"],
      "disableDynamicRegister": true,
      "trace.server": "verbose"
    }
  },
  "diagnostic-languageserver.filetypes": {
    "markdown": [
      "write-good",
      "markdownlint"
    ],
    "ruby": [
      "solargraph"
    ],
    "javascript": [
      "eslint"
    ],
    "typescript": [
      "eslint"
    ],
    "javascriptreact": [
      "eslint"
    ],
    "javascript.jsx": [
      "eslint"
    ],
    "typescriptreact": [
      "eslint"
    ],
    "typescript.jsx": [
      "eslint"
    ],
    "sh": "shellcheck"
  },
  "solargraph.diagnostics": true,
  "solargraph.hover": true,
  "solargraph.autoformat": true,
  "solargraph.useBundler": true,
  "solargraph.bundlerPath": "~/.rbenv/shims/bundle"
}
